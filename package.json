{
  "name": "chore-cli",
  "version": "0.0.7",
  "description": "Ô∏èChore CLI is the Tooling for building a front-end library.",
  "main": "dist/cli.js",
  "scripts": {
    "test": "jest",
    "build": "tsc --outDir dist",
    "dev": "tsc --outDir dist --watch"
  },
  "bin": {
    "chore": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iwfan/chore-cli.git"
  },
  "keywords": [
    "cli",
    "chore"
  ],
  "author": "iwfan <i.wangfancn@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iwfan/chore-cli/issues"
  },
  "homepage": "https://github.com/iwfan/chore-cli#readme",
  "engines": {
    "npm": ">=6.9.0",
    "node": ">=10.16.0"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,scss,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "execa": "^1.0.0",
    "fs-extra": "^8.0.1",
    "inquirer": "^6.3.1",
    "lint-staged": "^9.2.1",
    "ora": "^3.4.0",
    "prettier": "^1.18.2",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^7.0.0",
    "@types/inquirer": "^6.0.2",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/prettier": "^1.18.0",
    "@types/yargs": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
}
